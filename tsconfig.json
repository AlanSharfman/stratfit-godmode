{
    "compilerOptions": {
        "target": "ES2022",
        "useDefineForClassFields": true,
        "lib": [
            "ES2022",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
        "types": [
            "vite/client"
        ]
    },
    "include": [
        "src",
        "vite-env.d.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "_HARD_SAVES/**",
        "**/*HARD_SAVES*",
        "**/*LOCK_*",
        "**/*.lock.*",
        "**/*.bak.*",
        "src/_quarantine/**",
        "src/onboard/**",
        "src/pitch/**",
        "src/strategy/**",
        "src/components/compound/variances2/**",
        "src/components/compound/_variances2_DISABLED/**"
    ]
}